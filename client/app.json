{
  "expo": {
    "name": "BizK: Booking Client",
    "slug": "client",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.jpg",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/icon.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#f5f5f5"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "buildNumber": "1.0.1",
      "supportsTablet": true,
      "bundleIdentifier": "com.tester9977.client"
    },
    "android": {
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.jpg",
        "backgroundColor": "#ffffff"
      },
      "package": "com.tester9977.client"
    },
    "web": {
      "favicon": "./assets/icon.jpg"
    },
    "plugins": [
      "expo-font",
      [
        "expo-barcode-scanner",
        {
          "cameraPermission": "All to access camera"
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera",
          "microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone",
          "recordAudioAndroid": true
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "57a854a5-8078-409e-be9e-0cd265a2406a"
      }
    }
  }
}
